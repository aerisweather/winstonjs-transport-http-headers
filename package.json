    "debug"
  ],
  "author": "Seth Miller <smiller@aerisweather.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aerisweather/winstonjs-transport-http-headers/issues"
  },
  "homepage": "https://github.com/aerisweather/winstonjs-transport-http-headers#readme",
  "dependencies": {
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.0.0",
    "nock": "^10.0.6",
    "nyc": "^13.2.0",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2"
  }
