  "dependencies": {
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "express": "^4.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
