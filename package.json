{
  "name": "winstonjs-transport-http-headers",
  "version": "2.0.0",
  "description": "A WinstonJS transport that logs to HTTP headers, useful for sending profiling data to the browser that doesn't muddle the response body.",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerisweather/winstonjs-transport-http-headers.git"
  },
  "keywords": [
    "winston",
    "logging",
    "transport",
    "http",
    "headers",
    "profiling",
